{
  "name": "LawProj",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "browserify -t babelify --standalone main views/index/main.js -o public/js/index/main.js && browserify -t babelify --standalone main views/company/management/main.js -o public/js/company/management/main.js",
    "start": "node ./bin/www"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "react": "global:React",
    "react-dom": "global:ReactDOM",
    "jquery": "global:$",
    "bcrypt": "global:dcodeIO.bcrypt",
    "moment": "global:moment",
    "react-bootstrap-datetimepicker": "global:ReactBootstrapDatetimepicker",
    "react-data-components": "global:ReactDataComponents"
  },
  "dependencies": {
    "babelify": "^6.3.0",
    "bcrypt": "^0.8.6",
    "body-parser": "~1.13.2",
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.12",
    "connect-redis": "^3.0.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-react-views": "^0.10.1",
    "express-session": "~1.13.0",
    "hiredis": "^0.4.1",
    "jquery": "^2.2.3",
    "jsdom": "^9.0.0",
    "moment": "^2.13.0",
    "mongoose": "^4.4.14",
    "morgan": "~1.6.1",
    "node-schedule": "^1.1.0",
    "react": "^0.14.8",
    "react-bootstrap-datetimepicker": "0.0.22",
    "react-data-components": "^0.6.0",
    "react-dom": "^0.14.8",
    "redis": "^2.6.0-2",
    "request": "^2.72.0",
    "serve-favicon": "~2.3.0"
  }
}
