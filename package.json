    "jquery": "global:$",
    "bcrypt": "global:dcodeIO.bcrypt"
  },
  "dependencies": {
    "babelify": "^6.3.0",
    "bcrypt": "^0.8.6",
    "body-parser": "~1.13.2",
    "browserify": "^13.0.0",
