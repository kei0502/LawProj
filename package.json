{
  "name": "LawProj",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "browserify -t babelify --standalone main views/index/main.js -o public/js/index/main.js && node ./bin/www"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
