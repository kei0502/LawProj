    "connect-redis": "^3.0.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-react-views": "^0.10.1",
    "express-session": "~1.13.0",
    "hiredis": "^0.4.1",
    "jquery": "^2.2.3",
    "mongoose": "^4.4.14",
    "morgan": "~1.6.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "redis": "^2.6.0-2",
    "serve-favicon": "~2.3.0"
  }
}
